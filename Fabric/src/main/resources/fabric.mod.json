{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_full_name}",
  "description": "${mod_description}",
  "authors": [
    "YUNGNICKYOUNG"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/yungs-api-fabric",
    "discord": "discord.gg/rns3beq",
    "sources": "https://github.com/yungnickyoung/YUNGs-Api",
    "issues": "https://github.com/yungnickyoung/YUNGs-Api/issues"
  },
  "license": "${license}",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.yungnickyoung.minecraft.yungsapi.YungsApiFabric"
    ]
  },
  "mixins": [
    "yungsapi.mixins.json",
    "yungsapi_fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric": ">=${fabric_version}",
    "minecraft": ">=${mc_version}"
